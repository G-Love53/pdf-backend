    <div class="block">
      <div class="row">
        <div class="cell label w15">INCREASED LIMITS</div>
        <div class="cell w10 right">$<%= moneyUSD(S.increasedLimits) %></div>
        <div class="cell label w12">DEDUCTIBLE *</div>
        <div class="cell w10 right">$<%= moneyUSD(S.deductible) %></div>
