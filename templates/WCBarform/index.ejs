    .row:last-child { border-bottom:none; }
    .cell { border-right:1px solid #000; padding:1px 3px; display:flex; align-items:center; overflow-wrap:break-word; }
    .cell:last-child { border-right:none; }
    .cell.label { background:#f0f0f0; font-weight:bold; font-size:6.5pt; }
    .cell.center { justify-content:center; text-align:center; }
    .cell.right { justify-content:flex-end; text-align:right; }
    .cell.wrap { flex-wrap:wrap; align-items:flex-start; }
    .cell.small { font-size:7pt; }
    .cell.legal { font-size:6pt; line-height:1.2; }
    .w5{width:5%}.w6{width:6%}.w7{width:7%}.w8{width:8%}.w10{width:10%}.w12{width:12%}.w15{width:15%}.w18{width:18%}.w20{width:20%}.w22{width:22%}.w25{width:25%}.w27{width:27%}.w28{width:28%}.w30{width:30%}.w33{width:33%}.w35{width:35%}.w40{width:40%}.w45{width:45%}.w50{width:50%}.w55{width:55%}.w60{width:60%}.w65{width:65%}.w70{width:70%}.w75{width:75%}.w80{width:80%}.w85{width:85%}.w90{width:90%}.w95{width:95%}.w100{width:100%}
    .check { display:inline-block; width:10px; height:10px; border:1px solid #000; text-align:center; line-height:8px; font-size:8pt; margin-right:5px; vertical-align:middle; }
    .check-xs { width:8px; height:8px; font-size:6pt; line-height:6px; }
    .footnote { position:absolute; bottom:0.125in; left:0.125in; right:0.125in; font-size:6pt; border-top:1px solid #000; padding-top:2px; text-align:center; }
    @media screen { .page{margin-bottom:20px; box-shadow:0 0 10px rgba(0,0,0,.1)} }
  </style>
</head>
<body>

<!-- ====================== PAGE 1 ====================== -->
<div class="page">
  <div class="headerbar">
    <div class="acord">ACORD</div>
    <div class="title">WORKERS' COMPENSATION APPLICATION</div>
    <div class="formno">130</div>
  </div>

  <div class="subbar">PRODUCER / APPLICANT</div>
  <div class="block">
    <div class="row">
      <div class="cell label w15">AGENCY NAME AND ADDRESS</div>
      <div class="cell w35 wrap">
        <%= data.producerName %><br/>
        <%= data.producerAddress1 %><br/>
        <%= data.producerAddress2 %>
      </div>
      <div class="cell label w15">AGENCY CUSTOMER ID</div>
      <div class="cell w15"><%= data.agencyCustomerId || "" %></div>
      <div class="cell label w10">DATE (MM/DD/YYYY)</div>
      <div class="cell w10"><%= formatDate(data.date) %></div>
    </div>
    <div class="row">
      <div class="cell label w20">APPLICANT NAME</div>
      <div class="cell w30"><%= data.applicant_name || "" %></div>
      <div class="cell label w10">FEIN</div>
      <div class="cell w10"><%= data.fein %></div>
      <div class="cell label w10">WEBSITE</div>
      <div class="cell w20"><%= data.website %></div>
    </div>
    <div class="row">
      <div class="cell label w20">MAILING ADDRESS (including ZIP+4)</div>
      <div class="cell w35 wrap">
        <%= data.mailing_address1 || "" %><% if (data.mailing_address2) { %><br/><%= data.mailing_address2 %><% } %><br/>
        <%= [data.mailing_city, data.mailing_state, data.mailing_zip].filter(Boolean).join(", ") %>
      </div>
      <div class="cell label w10">CARRIER</div>
      <div class="cell w10"><%= data.carrier %></div>
      <div class="cell label w10">NAIC CODE</div>
      <div class="cell w15"><%= data.naicCode %></div>
    </div>
    <div class="row">
      <div class="cell label w15">POLICY NUMBER</div>
      <div class="cell w15"><%= data.policyNumber %></div>
      <div class="cell label w15">EFFECTIVE DATE</div>
      <div class="cell w15"><%= formatDate(data.effectiveDate) %></div>
      <div class="cell label w15">EXPIRATION DATE</div>
      <div class="cell w15"><%= formatDate(data.expirationDate) %></div>
    </div>
  </div>

  <div class="subbar">STATUS OF SUBMISSION</div>
  <div class="block">
    <div class="row">
      <div class="cell w25 small"><span class="check"><%= yes(data.status_quote) ? "X" : "" %></span> QUOTE</div>
      <div class="cell w25 small"><span class="check"><%= yes(data.status_issuePolicy) ? "X" : "" %></span> ISSUE POLICY</div>
      <div class="cell w25 small"><span class="check"><%= yes(data.status_bound) ? "X" : "" %></span> BOUND</div>
      <div class="cell w25 small"><span class="check"><%= yes(data.assignedRisk) ? "X" : "" %></span> ASSIGNED RISK (Attach ACORD 133)</div>
    </div>
  </div>

  <div class="subbar">BILLING / AUDIT INFORMATION</div>
  <div class="block">
    <div class="row">
      <div class="cell label w12">BILLING PLAN</div>
      <div class="cell w20 small">
        <span class="check"><%= yes(data.billingPlan_agencyBill) ? "X" : "" %></span> AGENCY BILL
        &nbsp;&nbsp;
        <span class="check"><%= yes(data.billingPlan_directBill) ? "X" : "" %></span> DIRECT BILL
      </div>
      <div class="cell label w12">PAYMENT PLAN</div>
      <div class="cell w18"><%= data.paymentPlan %></div>
      <div class="cell label w12">% DOWN</div>
      <div class="cell w10 right"><%= data.paymentPlan_downPct %></div>
      <div class="cell label w8">AUDIT</div>
      <div class="cell w20 small">
        <span class="check"><%= yes(data.audit_atExpiration) ? "X" : "" %></span> AT EXPIRATION
        <span class="check"><%= yes(data.audit_semiAnnual) ? "X" : "" %></span> SEMI-ANNUAL
        <span class="check"><%= yes(data.audit_quarterly) ? "X" : "" %></span> QUARTERLY
        <span class="check"><%= yes(data.audit_monthly) ? "X" : "" %></span> MONTHLY
      </div>
    </div>
  </div>

  <div class="subbar">LOCATIONS</div>
  <div class="block">
    <div class="row">
      <div class="cell label w8 center">LOC #</div>
      <div class="cell label w62 center">STREET, CITY, COUNTY, STATE, ZIP CODE</div>
      <div class="cell label w30 center">FLOOR HIGHEST</div>
    </div>
    <% (Array.isArray(data.locations)?data.locations:[]).forEach(function(L){ %>
      <div class="row">
        <div class="cell w8 center"><%= L.loc || "" %></div>
        <div class="cell w62 wrap">
          <%= [L.street, L.city, L.county, L.state, L.zip].filter(Boolean).join(", ") %>
        </div>
        <div class="cell w30 center"><%= L.floorHighest || "" %></div>
      </div>
    <% }) %>
  </div>

  <div class="subbar">POLICY INFORMATION</div>
  <div class="block">
    <div class="row">
      <div class="cell label w15">PROPOSED EFF DATE</div>
      <div class="cell w10"><%= formatDate(data.proposedEffDate) %></div>
      <div class="cell label w15">PROPOSED EXP DATE</div>
      <div class="cell w10"><%= formatDate(data.proposedExpDate) %></div>
      <div class="cell label w15">RATING EFFECTIVE DATE</div>
      <div class="cell w10"><%= formatDate(data.ratingEffectiveDate) %></div>
      <div class="cell label w15">ANNIVERSARY RATING DATE</div>
      <div class="cell w10"><%= formatDate(data.anniversaryRatingDate) %></div>
    </div>
    <div class="row">
      <div class="cell label w15">RETRO PLAN</div>
      <div class="cell w35 small">
        <span class="check"><%= yes(data.retroPlan_participating) ? "X" : "" %></span> PARTICIPATING
        &nbsp;&nbsp;
        <span class="check"><%= yes(data.retroPlan_nonParticipating) ? "X" : "" %></span> NON-PARTICIPATING
      </div>
      <div class="cell label w15">DEDUCTIBLES (N/A WI)</div>
      <div class="cell w35 small">
        MEDICAL $<%= moneyUSD(data.deduct_medical) %> &nbsp;&nbsp; INDEMNITY $<%= moneyUSD(data.deduct_indemnity) %>
      </div>
    </div>
  </div>

  <div class="subbar">COVERAGE</div>
  <div class="block">
    <div class="row">
      <div class="cell label w25">PART 1 â€” WORKERS COMPENSATION (STATES)</div>
      <div class="cell w75 wrap"><%= (Array.isArray(data.wc_coveredStates)?data.wc_coveredStates:[]).join(", ") %></div>
    </div>
    <div class="row">
      <div class="cell label w25">PART 2 â€” EMPLOYER'S LIABILITY</div>
      <div class="cell label w25">EACH ACCIDENT</div>
      <div class="cell w10 right">$<%= data.el_eachAccident %></div>
      <div class="cell label w20">DISEASE â€” POLICY LIMIT</div>
      <div class="cell w10 right">$<%= data.el_diseasePolicyLimit %></div>
      <div class="cell label w10">DISEASE â€” EACH EMP</div>
      <div class="cell w10 right">$<%= data.el_diseaseEachEmployee %></div>
    </div>
    <div class="row">
      <div class="cell label w25">PART 3 â€” OTHER STATES</div>
      <div class="cell w75 wrap"><%= data.otherStatesText %></div>
    </div>
    <div class="row">
      <div class="cell label w25">ADDITIONAL COMPANY INFORMATION</div>
      <div class="cell w75 small">
        DIVIDEND PLAN / SAFETY GROUP: <%= data.dividendPlan_safetyGroup || "" %>
        &nbsp;&nbsp; <span class="check"><%= yes(data.cov_uslh) ? "X" : "" %></span> U.S.L.&H.
        &nbsp;&nbsp; <span class="check"><%= yes(data.cov_foreignVoluntary) ? "X" : "" %></span> FOREIGN VOL COMP
        &nbsp;&nbsp; <span class="check"><%= yes(data.cov_managedCare) ? "X" : "" %></span> MANAGED CARE OPTION
      </div>
    </div>
    <div class="row">
      <div class="cell label w35">SPECIFY ADDITIONAL COVERAGES / ENDORSEMENTS</div>
      <div class="cell w65 wrap"><%= data.additionalCoveragesText || "" %></div>
    </div>
  </div>

  <div class="subbar">CONTACT INFORMATION</div>
  <div class="block">
    <div class="row">
      <div class="cell label w15 center">TYPE</div>
      <div class="cell label w25 center">NAME</div>
      <div class="cell label w20 center">OFFICE PHONE</div>
      <div class="cell label w20 center">MOBILE PHONE</div>
      <div class="cell label w20 center">E-MAIL</div>
    </div>
    <% const CI = [
         { t:"INFO", c:data.contact_info },
         { t:"CLAIMS", c:data.contact_claims },
         { t:"RECORD / ACCTNG", c:data.contact_accounting },
         { t:"INSPECTION", c:data.contact_inspection }
       ]; 
       CI.forEach(function(x){ %>
      <div class="row">
        <div class="cell w15 center"><%= x.t %></div>
        <div class="cell w25"><%= x.c?.name || "" %></div>
        <div class="cell w20"><%= x.c?.officePhone || "" %></div>
        <div class="cell w20"><%= x.c?.mobile || "" %></div>
        <div class="cell w20"><%= x.c?.email || "" %></div>
      </div>
    <% }) %>
  </div>

  <div class="subbar">INDIVIDUALS INCLUDED / EXCLUDED</div>
  <div class="block">
    <div class="row">
      <div class="cell label w20 center">NAME / TITLE</div>
      <div class="cell label w10 center">DOB</div>
      <div class="cell label w10 center">% OWNERSHIP</div>
      <div class="cell label w15 center">DUTIES</div>
      <div class="cell label w8 center">INC</div>
      <div class="cell label w8 center">EXC</div>
      <div class="cell label w9 center">CLASS CODE</div>
      <div class="cell label w10 center">REMUN/PAYROLL</div>
      <div class="cell label w5 center">LOC</div>
      <div class="cell label w5 center">STATE</div>
    </div>
    <% (Array.isArray(data.officers)?data.officers:[]).forEach(function(O){ %>
      <div class="row">
        <div class="cell w20 wrap"><%= [O.name, O.title].filter(Boolean).join(" â€” ") %></div>
        <div class="cell w10 center"><%= O.dob || "" %></div>
        <div class="cell w10 center"><%= O.ownershipPct || "" %></div>
        <div class="cell w15 wrap"><%= O.duties || "" %></div>
        <div class="cell w8 center"><%= yes(O.include) ? "X" : "" %></div>
        <div class="cell w8 center"><%= yes(O.include) ? "" : "X" %></div>
        <div class="cell w9 center"><%= O.classCode || "" %></div>
        <div class="cell w10 right">$<%= moneyUSD(O.remuneration) %></div>
        <div class="cell w5 center"><%= O.loc || "" %></div>
        <div class="cell w5 center"><%= O.state || "" %></div>
      </div>
    <% }) %>
  </div>

  <div class="footnote">ACORD 130 (2017/05) &nbsp;&nbsp;&nbsp; Page 1 of 4 &nbsp;&nbsp;&nbsp; Â© ACORD CORPORATION. All rights reserved.</div>
</div>

<!-- ====================== PAGE 2: STATE RATING WORKSHEET ====================== -->
