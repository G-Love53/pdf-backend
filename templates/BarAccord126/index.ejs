  ebl_retroDate: "",

  // Claims Made (if gl_form === "CLAIMS_MADE")
  cm_proposedRetroDate: "",
  cm_entryDateUninterrupted: "",
  cm_priorExclusions: false,
  cm_tailPurchased: false,
  cm_explainYes: "",

  // Products / Completed Ops (table of products)
  products: [],
