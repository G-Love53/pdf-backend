<!doctype html>
<html>
<head>
  <meta charset="utf-8"/>
  <title>Society – Special Program Supplemental Application</title>
  <style>
    /* Embedded print-like styling (similar to your 125 look) */
    @page { size: Letter; margin: 36pt; }
    body { font-family: Arial, Helvetica, sans-serif; color:#111; }
    .page { page-break-inside: avoid; }
    .pagebreak { page-break-before: always; }
    .carrierbar {
      display:flex; align-items:center; justify-content:space-between;
      border: 2px solid #000; padding:10pt 12pt; margin-bottom:10pt;
      background:#f5f5f5;
    }
    .carrierbar .brand { font-weight:700; letter-spacing:0.5pt; }
    .carrierbar .title { font-weight:700; }
    .subbar {
      background:#000; color:#fff; font-weight:700; padding:6pt 8pt;
      margin:14pt 0 6pt; letter-spacing:0.2pt;
    }
    .block { border:1px solid #000; padding:8pt; }
    .row { display:flex; flex-wrap:wrap; border-bottom:1px solid #ddd; }
    .row:last-child { border-bottom:none; }
    .cell { padding:6pt 6pt; box-sizing:border-box; }
    .label { font-weight:700; background:#fafafa; border-right:1px solid #eee; }
    .w10{width:10%}.w15{width:15%}.w20{width:20%}.w25{width:25%}.w30{width:30%}
    .w35{width:35%}.w40{width:40%}.w50{width:50%}.w60{width:60%}.w70{width:70%}
    .w75{width:75%}.w80{width:80%}.w90{width:90%}.w100{width:100%}
    .right { text-align:right; }
    .check {
      display:inline-block; min-width:14pt; text-align:center; border:1px solid #000; margin-right:6pt; font-weight:700;
      width:14pt; height:14pt; line-height:12pt; vertical-align:middle; font-size:10pt;
    }
    .muted { color:#666; }
    .legal { font-size:10pt; line-height:1.3; }
  </style>
</head>
<body>
<% const { isYes, yesno, yn, money, moneyUSD, formatDate, join, calcAlcoholPercent } = helpers; %>

<!-- ===== PAGE 1 ===== -->
<div class="page">
  <div class="carrierbar">
    <div class="brand">SOCIETY INSURANCE</div>
    <div class="title">SPECIAL PROGRAM SUPPLEMENTAL APPLICATION</div>
  </div>

  <!-- APPLICANT -->
  <div class="subbar">APPLICANT</div>
  <div class="block">
    <div class="row">
      <div class="cell label w25">BUSINESS NAME</div>
      <div class="cell w75"><%= data.applicant_name || data.premises_name || "" %></div>
    </div>
    <div class="row">
      <div class="cell label w25">LOCATION</div>
      <div class="cell w75">
        <%= join([data.premise_address, data.premise_city, data.premise_state, data.premise_zip]) %>
      </div>
    </div>
    <div class="row">
      <div class="cell label w25">PHONE / EMAIL</div>
      <div class="cell w75"><%= join([data.business_phone, data.contact_email], " / ") %></div>
    </div>
    <div class="row">
      <div class="cell label w25">EFFECTIVE DATE</div>
      <div class="cell w25"><%= data.effective_date || "" %></div>
      <div class="cell label w25">ORGANIZATION TYPE</div>
      <div class="cell w25"><%= data.organization_type || "" %></div>
    </div>
  </div>

  <!-- ELIGIBILITY -->
  <div class="subbar">ELIGIBILITY</div>
  <div class="block">
    <div class="row">
      <div class="cell label w60">1. Open now or within 60 days?</div>
      <div class="cell w40"><span class="check"><%= isYes(data.open_60_days) ? "✓" : "" %></span> Yes</div>
    </div>
    <% if (!isYes(data.open_60_days) && data.open_60_days_details) { %>
      <div class="row">
        <div class="cell label w30">If No, details</div>
        <div class="cell w70"><%= data.open_60_days_details %></div>
      </div>
    <% } %>

    <div class="row">
      <div class="cell label w60">2. 3+ yrs restaurant/bar ownership experience in past 5 yrs?</div>
      <div class="cell w40"><span class="check"><%= isYes(data.ownership_experience) ? "✓" : "" %></span> Yes</div>
    </div>
    <% if (data.ownership_experience_details || data.ownership_experience_details_yes || data.ownership_experience_details_no) { %>
      <div class="row">
        <div class="cell label w30">Experience details</div>
        <div class="cell w70">
          <%= data.ownership_experience_details 
              || (isYes(data.ownership_experience) ? data.ownership_experience_details_yes : data.ownership_experience_details_no) 
              || "" %>
        </div>
      </div>
    <% } %>
  </div>

  <!-- OPERATIONS -->
  <div class="subbar">OPERATIONS</div>
  <div class="block">
    <div class="row">
      <div class="cell label w25">3. Closing time</div>
      <div class="cell w25"><%= data.closing_time || "" %></div>
      <div class="cell label w25">4. Square footage</div>
      <div class="cell w25"><%= data.square_footage || "" %></div>
    </div>
    <div class="row">
      <div class="cell label w25">5. # of employees</div>
      <div class="cell w25"><%= data.num_employees || "" %></div>
      <div class="cell label w25">6. Fine dining?</div>
      <div class="cell w25"><span class="check"><%= isYes(data.fine_dining) ? "✓" : "" %></span> Yes</div>
    </div>
    <div class="row">
      <div class="cell label w25">7. Counter service?</div>
      <div class="cell w25"><span class="check"><%= isYes(data.counter_service) ? "✓" : "" %></span> Yes</div>
      <div class="cell label w25">Automatic sprinkler</div>
      <div class="cell w25"><%= yesno(data.automatic_sprinkler) %></div>
    </div>
  </div>

  <!-- ALCOHOL & RECEIPTS -->
  <div class="subbar">ALCOHOL & RECEIPTS</div>
  <div class="block">
    <div class="row">
      <div class="cell label w40">8. Manufacture alcohol?</div>
      <div class="cell w10"><span class="check"><%= isYes(data.alcohol_manufactured) ? "✓" : "" %></span> Yes</div>
      <div class="cell label w35">If yes, >25% consumed on premises?</div>
      <div class="cell w15"><%= yesno(data.percent_consumed) %></div>
    </div>
    <div class="row">
      <div class="cell label w25">Food Sales</div>
      <div class="cell w25"><%= moneyUSD(data.food_sales) %></div>
      <div class="cell label w25">Alcohol Sales</div>
      <div class="cell w25"><%= moneyUSD(data.alcohol_sales) %></div>
    </div>
    <div class="row">
      <div class="cell label w25">Total Sales</div>
      <div class="cell w25"><%= moneyUSD(data.total_sales) %></div>
      <div class="cell label w25">% Alcohol</div>
      <div class="cell w25"><%= data.Percent_Alcohol || calcAlcoholPercent(data.food_sales, data.alcohol_sales) %></div>
    </div>

    <% if (isYes(data.alcohol_manufactured) && isYes(data.percent_consumed) && data.percent_consumed_details) { %>
      <div class="row">
        <div class="cell label w30">>25% consumed on premises – details</div>
        <div class="cell w70"><%= data.percent_consumed_details %></div>
      </div>
    <% } %>
  </div>

  <!-- COOKING & FIRE CONTROLS -->
  <div class="subbar">COOKING & FIRE CONTROLS</div>
  <div class="block">
    <div class="row">
      <div class="cell label w30">10. Level of cooking</div>
      <div class="cell w70">
        <span class="check"><%= isYes(data.cooking_level_full) ? "✓" : "" %></span> Full
        &nbsp;&nbsp;
        <span class="check"><%= (!isYes(data.cooking_level_full) && isYes(data.cooking_level_limited)) ? "✓" : "" %></span> Limited
        &nbsp;&nbsp;
        <span class="check"><%= (!isYes(data.cooking_level_full) && !isYes(data.cooking_level_limited) && isYes(data.cooking_level_non)) ? "✓" : "" %></span> None
      </div>
    </div>
    <div class="row">
      <div class="cell label w30">11. Cannabis infusion?</div>
      <div class="cell w20"><%= yesno(data.infused_with_cannabis) %></div>
      <div class="cell label w30">12. Solid fuel within 10 ft?</div>
      <div class="cell w20"><%= yesno(data.solid_fuel) %></div>
    </div>
    <div class="row">
      <div class="cell label w60">13. Any surfaces NOT protected by UL300 suppression?</div>
      <div class="cell w40"><%= yesno(data.non_UL300) %></div>
    </div>

    <!-- Grill / Smoker follow-up -->
    <% if (isYes(data.solid_fuel) && data.solid_fuel_details) { %>
      <div class="row">
        <div class="cell label w30">Grill / Smoker details</div>
        <div class="cell w70"><%= data.solid_fuel_details %></div>
      </div>
    <% } else if (isYes(data.solid_fuel)) { %>
      <div class="row">
        <div class="cell label w30">Grill / Smoker in use</div>
        <div class="cell w70">Yes (solid fuel within 10 ft)</div>
      </div>
    <% } %>
  </div>

  <!-- ENTERTAINMENT & RECREATION -->
  <div class="subbar">ENTERTAINMENT & RECREATION</div>
  <div class="block">
    <div class="row">
      <div class="cell label w35">14. Entertainment other than ambience/background, karaoke, trivia?</div>
      <div class="cell w15"><%= yesno(data.entertainment_other) %></div>
      <div class="cell label w15">Details</div>
      <div class="cell w35"><%= data.entertainment_details || "" %></div>
    </div>
    <% if (isYes(data.entertainment_other) && data.entertainment_details) { %>
      <div class="row">
        <div class="cell label w30">Entertainment details (expanded)</div>
        <div class="cell w70"><%= data.entertainment_details %></div>
      </div>
    <% } %>

    <div class="row">
      <div class="cell label w35">15. Recreational activities (besides darts, bag toss, ≤5 pool, horseshoes, volleyball)?</div>
      <div class="cell w15"><%= yesno(data.recreational_activites) %></div>
      <div class="cell label w15">Details</div>
      <div class="cell w35"><%= data.recreational_details || "" %></div>
    </div>
    <% if (isYes(data.recreational_activites) && data.recreational_details) { %>
      <div class="row">
        <div class="cell label w30">Recreational details (expanded)</div>
        <div class="cell w70"><%= data.recreational_details %></div>
      </div>
    <% } %>
  </div>

  <!-- SECURITY -->
  <div class="subbar">SECURITY</div>
  <div class="block">
    <div class="row">
      <div class="cell label w25">16. Bouncers / security present?</div>
      <div class="cell w25"><%= yesno(data.security_present) %></div>
      <div class="cell label w25">Background checks?</div>
      <div class="cell w25"><%= yesno(data.ComboBox22) %></div>
    </div>
    <div class="row">
      <div class="cell label w25">Armed?</div>
      <div class="cell w25"><%= yesno(data.ComboBox23) %></div>
      <div class="cell label w25">Conflict-resolution trained?</div>
      <div class="cell w25"><%= yesno(data.ComboBox24) %></div>
    </div>

    <% if (isYes(data.security_present)) { %>
      <div class="row">
        <div class="cell label w30">Bouncer/Security summary</div>
        <div class="cell w70">
          <%= [
                `Background checks: ${yesno(data.ComboBox22)}`,
                `Armed: ${yesno(data.ComboBox23)}`,
                `Conflict training: ${yesno(data.ComboBox24)}`
              ].join("  |  ") %>
        </div>
      </div>
    <% } %>

    <% if (isYes(data.security_present) && (data.security_count || data.security_hours)) { %>
      <div class="row">
        <div class="cell label w30">Security staffing</div>
        <div class="cell w70">
          <%= [
                data.security_count ? `Count: ${data.security_count}` : "",
                data.security_hours ? `Hours: ${data.security_hours}` : ""
              ].filter(Boolean).join("  |  ") %>
        </div>
      </div>
    <% } %>
  </div>

  <!-- Optional: SECURITY & CONTROLS (Door/TIPS/Incident) -->
  <% if (typeof data.id_checking !== "undefined" ||
         typeof data.tips_training !== "undefined" ||
         typeof data.servsafe !== "undefined" ||
         typeof data.incident_log !== "undefined") { %>
    <div class="subbar">SECURITY & CONTROLS</div>
    <div class="block">
      <% if (typeof data.id_checking !== "undefined") { %>
        <div class="row">
          <div class="cell label w40">ID check at door</div>
          <div class="cell w60"><%= yesno(data.id_checking) %></div>
        </div>
      <% } %>

      <% if (typeof data.tips_training !== "undefined" || typeof data.servsafe !== "undefined") { %>
        <div class="row">
          <div class="cell label w40">Server training (TIPS/ServSafe)</div>
          <div class="cell w60">
            <% const hasTips = typeof data.tips_training !== "undefined" ? yesno(data.tips_training) : ""; %>
            <% const hasSS  = typeof data.servsafe      !== "undefined" ? yesno(data.servsafe)      : ""; %>
            <%= [ hasTips ? `TIPS: ${hasTips}` : "", hasSS ? `ServSafe: ${hasSS}` : "" ].filter(Boolean).join("  |  ") %>
          </div>
        </div>
      <% } %>

      <% if (typeof data.incident_log !== "undefined") { %>
        <div class="row">
          <div class="cell label w40">Incident log maintained</div>
          <div class="cell w60"><%= yesno(data.incident_log) %></div>
        </div>
      <% } %>
    </div>
  <% } %>
</div>

<!-- ===== PAGE 2 ===== -->
<div class="page pagebreak">
  <div class="carrierbar">
    <div class="brand">SOCIETY INSURANCE</div>
    <div class="title">SPECIAL PROGRAM SUPPLEMENTAL APPLICATION</div>
  </div>

  <!-- DELIVERY -->
  <div class="subbar">DELIVERY</div>
  <div class="block">
    <div class="row">
      <div class="cell label w25">17. Delivery offered?</div>
      <div class="cell w25"><%= yesno(data.delivery_offered) %></div>
      <div class="cell label w25">Owned autos?</div>
      <div class="cell w25"><%= yesno(data.ComboBox13) %></div>
    </div>
    <div class="row">
      <div class="cell label w25">Employee autos?</div>
      <div class="cell w25"><%= yesno(data.ComboBox14) %></div>
      <div class="cell label w25">3rd-party services?</div>
      <div class="cell w25"><%= yesno(data.ComboBox15) %></div>
    </div>
    <div class="row">
      <div class="cell label w40">Delivery sales (owned or employee autos)</div>
      <div class="cell w20"><%= moneyUSD(data.TextField0) %></div>
      <div class="cell label w25">>20% at any one location?</div>
      <div class="cell w15"><%= yesno(data.ComboBox16) %></div>
    </div>
    <% if (isYes(data.ComboBox16) && data.TextField12) { %>
      <div class="row">
        <div class="cell label w30">If >20% at one location, explain</div>
        <div class="cell w70"><%= data.TextField12 %></div>
      </div>
    <% } %>

    <div class="row">
      <div class="cell label w25">Radius > 5 miles?</div>
      <div class="cell w25"><%= yesno(data.ComboBox17) %></div>
      <div class="cell label w25">Hours past 10 PM?</div>
      <div class="cell w25"><%= yesno(data.ComboBox18) %></div>
    </div>
    <% if (isYes(data.ComboBox17) && data.TextField11) { %>
      <div class="row">
        <div class="cell label w30">Delivery radius explanation</div>
        <div class="cell w70"><%= data.TextField11 %></div>
      </div>
    <% } %>
    <% if (isYes(data.ComboBox18) && data.TextField13) { %>
      <div class="row">
        <div class="cell label w30">After 10 PM delivery explanation</div>
        <div class="cell w70"><%= data.TextField13 %></div>
      </div>
    <% } %>

    <% if (isYes(data.delivery_offered)) { %>
      <div class="row">
        <div class="cell label w30">Delivery methods</div>
        <div class="cell w70">
          <%= [
                isYes(data.ComboBox13) ? "Owned autos"     : "",
                isYes(data.ComboBox14) ? "Employee autos"  : "",
                isYes(data.ComboBox15) ? "3rd-party svc"   : ""
              ].filter(Boolean).join("  |  ") %>
        </div>
      </div>
    <% } %>
  </div>

  <!-- AUTO / SHUTTLE -->
  <div class="subbar">AUTO / SHUTTLE</div>
  <div class="block">
    <div class="row">
      <div class="cell label w40">18. If auto rated, does business provide shuttle?</div>
      <div class="cell w20"><%= yesno(data.ComboBox19) %></div>
      <div class="cell label w15">Details</div>
      <div class="cell w25"><%= data.shuttle_explanation || "" %></div>
    </div>
    <% if (isYes(data.ComboBox19) && data.shuttle_explanation) { %>
      <div class="row">
        <div class="cell label w30">Shuttle details (expanded)</div>
        <div class="cell w70"><%= data.shuttle_explanation %></div>
      </div>
    <% } %>
    <div class="row">
      <div class="cell label w60">Other auto policies in force for the named insured(s)?</div>
      <div class="cell w40"><%= yesno(data.ComboBox1) %></div>
    </div>
  </div>

  <!-- VIOLATIONS & CLAIMS -->
  <div class="subbar">LIQUOR VIOLATIONS & CLAIMS (Last 3 Years)</div>
  <div class="block">
    <div class="row">
      <div class="cell label w40">19. Liquor law violations?</div>
      <div class="cell w20"><%= yesno(data.liquor_lapse) %></div>
      <div class="cell w40"><%= data.liquor_claims || "" %></div>
    </div>
    <div class="row">
      <div class="cell label w30">Total claims</div>
      <div class="cell w20"><%= data.claim_count || "" %></div>
      <div class="cell w50">
        <% if (data.claim_count==="2_or_less" && data.claims_details_2_or_less) { %>
          <%= data.claims_details_2_or_less %>
        <% } else if (data.claim_count==="3_or_more" && data.claims_details_3_or_more) { %>
          <%= data.claims_details_3_or_more %>
        <% } %>
      </div>
    </div>
  </div>

  <!-- PAYMENT PLAN -->
  <div class="subbar">PAYMENT PLAN</div>
  <div class="block">
    <div class="row">
      <div class="cell w50"><span class="check"><%= isYes(data.payment_plan_Monthly) ? "✓" : "" %></span> Monthly</div>
      <div class="cell w50"><span class="check"><%= isYes(data.payment_plan_Annual) ? "✓" : "" %></span> Annual</div>
    </div>
  </div>

  <!-- AGENCY FOOTER -->
  <div class="subbar">AGENCY</div>
  <div class="block">
    <div class="row">
      <div class="cell label w25">Agency</div>
      <div class="cell w75"><%= data.TextField16 || "All Access Insurance dba Commercial Insurance Direct, LLC 70025M" %></div>
    </div>
    <div class="row">
      <div class="cell label w25">Agent</div>
      <div class="cell w25"><%= data.TextField17 || "Rick Cline" %></div>
      <div class="cell label w25">Agent Email</div>
      <div class="cell w25"><%= data.TextField18 || "quote@barinsurancedirect.com" %></div>
    </div>
    <div class="row">
      <div class="cell label w25">Agent Phone</div>
      <div class="cell w25"><%= data.TextField19 || "303-932-1700" %></div>
      <div class="cell label w25">Date</div>
      <div class="cell w25"><%= data.Date || "" %></div>
    </div>
  </div>

  <!-- SIGNATURE -->
  <div class="subbar">SIGNATURE</div>
  <div class="block">
    <div class="row">
      <div class="cell label w30">Applicant’s Signature</div>
      <div class="cell w70">&nbsp;</div>
    </div>
    <div class="row">
      <div class="cell label w30">Title</div>
      <div class="cell w30">&nbsp;</div>
      <div class="cell label w20">Date</div>
      <div class="cell w20"><%= data.Date || "" %></div>
    </div>
    <div class="row">
      <div class="cell w100">
        <div class="legal">The applicant affirms the accuracy and completeness of the information provided…</div>
      </div>
    </div>
  </div>
</div>

</body>
</html>


