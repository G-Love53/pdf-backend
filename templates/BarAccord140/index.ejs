<%
/* ================= ACORD 140 — PROPERTY SECTION (3 pages) =================
   Mirrors legacy BarAccord140.pdf for cross-segment reuse.
============================================================================ */

const defaults = {
  // Producer / Agency
  producerName: "All Access Ins, dba Commercial Insurance Direct LLC",
  producerAddress1: "9200 W Cross Drive #515",
  producerAddress2: "Littleton, CO 80123",
  producerContact: "Rick Cline",
  producerPhone: "303-932-1700",
  producerEmail: "quote@barinsurancedirect.com",

  // Header meta
  date: "",
  carrier: "",
  naicCode: "",
  policyNumber: "",
  agencyCustomerId: "",

  // Named insured / premises basics
  named_insured: "",
  applicant_name: "",
  premises_name: "1",
  premise_address: "",
  premise_city: "",
  premise_state: "",
  premise_zip: "",
  building_number: "1",
  building_description: "",

  // Subject of Insurance (hard-code asks)
  business_personal_property: "", // prints in SOI (DED $2,500)
  building_quote: "",             // prints in SOI

  // Construction / property info
  construction_type: "",
  number_of_stories: "",
  year_built: "",
  square_footage: "",
  automatic_sprinkler: "", // Yes/No or %
  roof_type: "",
  number_of_basements: "",
  protection_class: "",
  distance_hydrant_ft: "",
  distance_firestation_mi: "",
  wind_class: "",
  building_grade: "",
  building_code: "",
  other_occupancies: "",

  // Protection / alarms
  fire_alarm_type: "",
  fire_alarm_monitor: "",
  fire_alarm_manufacturer: "",
  burglar_alarm_type: "",
  burglar_cert_number: "",
  burglar_cert_expiration: "",
  burglar_alarm_installer: "",
  burglar_alarm_keys: "",
  guards_watchmen: "",
  guard_clock_hourly: "",

  // Business income / value reporting
  business_income_form: "",
  business_income_amount: "",
  business_income_waiting_period: "",
  business_income_monthly_limit: "",
  business_income_extended: "",

  value_reporting_basis: "",
  value_reporting_frequency: "",
  value_reporting_provisional_pct: "",

  addl_coverages_options_restrictions: "",
  additional_information: "",
  remarks: "",

  // Additional Interest (page 2 per your instruction)
  interests: [
    // { type:"ADDITIONAL_INSURED|LOSS_PAYEE|MORTGAGEE|LIENHOLDER|LENDERS_LOSS",
    //   name:"", address:"", rank:"", loanNumber:"",
    //   itemType:"LOCATION|BUILDING|VEHICLE|BOAT", itemDescription:"" }
  ],

  // Signature defaults
  producerSignatureName: "Rick Cline",
  producerStateLicense: "",
  producerNPN: ""
};

const data = Object.assign({}, defaults, (typeof formData === "object" && formData) || {});

// ===== Helpers (tiny, safe) =====
const yes = (v) => {
  const s = String(v ?? "").toLowerCase();
  return ["yes","true","1","on","checked","y"].includes(s) || v === true || v === 1;
};
const yn  = (v) => (v===null||v===undefined||v==="") ? "" : (yes(v) ? "Y" : "N");
const money = (v, opts={min:0,max:0}) => {
  if (v === null || v === undefined || v === "") return "";
  const n = Number(String(v).replace(/[^0-9.-]/g,""));
  if (Number.isNaN(n)) return String(v);
  return n.toLocaleString("en-US", { minimumFractionDigits: opts.min, maximumFractionDigits: opts.max });
};
const moneyUSD = (v) => money(v);
const formatDate = (d) => {
  try {
    if (!d) return "";
    const dt = new Date(d);
    if (Number.isNaN(dt.getTime())) return String(d);
    const mm = String(dt.getMonth()+1).padStart(2,"0");
    const dd = String(dt.getDate()).padStart(2,"0");
    const yyyy = dt.getFullYear();
    return `${mm}/${dd}/${yyyy}`;
  } catch { return String(d ?? ""); }
};
%>
<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8"/>
  <title>ACORD 140 - Property Section</title>
  <style>
    @media print { @page { margin: 0.25in; size: letter; } .page{ page-break-after: always; } .page:last-child{ page-break-after:auto; } }
    *{ box-sizing:border-box; margin:0; padding:0; }
    body{ font-family: Arial, sans-serif; font-size:8pt; line-height:1.1; color:#000; background:#fff; }
    .page{ width:100%; min-height:10.5in; padding:0.125in; border:1px solid #ccc; position:relative; }
    .headerbar{ display:flex; border:2px solid #000; background:#fff; margin-bottom:2px; align-items:center; height:40px; }
    .headerbar .acord, .headerbar .formno{ background:#000; color:#fff; font-weight:bold; font-size:16pt; width:60px; text-align:center; height:100%; display:flex; align-items:center; justify-content:center; }
    .headerbar .acord{ width:80px; }
    .headerbar .title{ flex:1; font-weight:bold; font-size:12pt; text-align:center; padding:0 10px; }
    .subbar{ background:#000; color:#fff; font-weight:bold; font-size:9pt; padding:3px 8px; margin:2px 0; text-align:left; }
    .block{ border:1px solid #000; margin-bottom:2px; }
    .row{ display:flex; border-bottom:1px solid #000; min-height:18px; }
    .row:last-child{ border-bottom:none; }
    .cell{ border-right:1px solid #000; padding:2px 3px; display:flex; align-items:center; overflow-wrap:anywhere; }
    .cell:last-child{ border-right:none; }
    .cell.label{ background:#f0f0f0; font-weight:bold; font-size:6.7pt; }
    .cell.center{ justify-content:center; text-align:center; }
    .cell.right{ justify-content:flex-end; text-align:right; }
    .cell.wrap{ flex-wrap:wrap; align-items:flex-start; }
    .cell.small{ font-size:7pt; }
    .cell.legal{ font-size:6pt; line-height:1.2; }
    .w5{width:5%}.w6{width:6%}.w7{width:7%}.w8{width:8%}.w9{width:9%}.w10{width:10%}.w12{width:12%}.w14{width:14%}.w15{width:15%}.w18{width:18%}.w20{width:20%}.w22{width:22%}.w25{width:25%}.w28{width:28%}.w30{width:30%}.w33{width:33%}.w35{width:35%}.w40{width:40%}.w45{width:45%}.w50{width:50%}.w55{width:55%}.w60{width:60%}.w65{width:65%}.w70{width:70%}.w75{width:75%}.w80{width:80%}.w85{width:85%}.w90{width:90%}.w100{width:100%}
    .check{ display:inline-block; width:10px; height:10px; border:1px solid #000; text-align:center; line-height:8px; font-size:8pt; margin-right:4px; vertical-align:middle; }
    .check-xs{ width:8px; height:8px; font-size:6pt; line-height:6px; }
    .footnote{ position:absolute; bottom:0.125in; left:0.125in; right:0.125in; font-size:6pt; border-top:1px solid #000; padding-top:2px; text-align:center; }
    @media screen {.page{margin-bottom:20px; box-shadow:0 0 10px rgba(0,0,0,.1)}}
  </style>
</head>
<body>

<!-- ====================== PAGE 1 ====================== -->
<div class="page">
  <div class="headerbar">
    <div class="acord">ACORD</div>
    <div class="title">PROPERTY SECTION — Attach to ACORD 125</div>
    <div class="formno">140</div>
  </div>

  <!-- HEADER LINE -->
  <div class="block">
    <div class="row">
      <div class="cell label w20">AGENCY</div>
      <div class="cell w35 wrap">
        <%= data.producerName %><br/>
        <%= data.producerAddress1 %><br/>
        <%= data.producerAddress2 %>
      </div>
      <div class="cell label w15">AGENCY CUSTOMER ID</div>
      <div class="cell w15"><%= data.agencyCustomerId || "" %></div>
      <div class="cell label w8">DATE</div>
      <div class="cell w7 center"><%= formatDate(data.date) %></div>
    </div>
    <div class="row">
      <div class="cell label w20">PROPERTY SECTION</div>
      <div class="cell w20">&nbsp;</div>
      <div class="cell label w10">CARRIER</div>
      <div class="cell w15"><%= data.carrier %></div>
      <div class="cell label w10">NAIC CODE</div>
      <div class="cell w10"><%= data.naicCode %></div>
      <div class="cell label w8">POLICY #</div>
      <div class="cell w7"><%= data.policyNumber %></div>
    </div>
    <div class="row">
      <div class="cell label w20">NAMED INSURED(S)</div>
      <div class="cell w80"><%= data.named_insured || data.applicant_name || "" %></div>
    </div>
  </div>

  <!-- PREMISES INFORMATION -->
  <div class="subbar">PREMISES INFORMATION</div>
  <div class="block">
    <div class="row">
      <div class="cell label w12">PREMISES #</div>
      <div class="cell w8 center"><%= data.premises_name || "1" %></div>
      <div class="cell label w12">BUILDING #</div>
      <div class="cell w8 center"><%= data.building_number || "1" %></div>
      <div class="cell label w20">STREET ADDRESS</div>
      <div class="cell w40 wrap"><%= [data.premise_address, data.premise_city, data.premise_state, data.premise_zip].filter(Boolean).join(", ") %></div>
    </div>
    <div class="row">
      <div class="cell label w20">BUILDING DESCRIPTION</div>
      <div class="cell w80 wrap"><%= data.building_description || "" %></div>
    </div>
  </div>

  <!-- SUBJECT OF INSURANCE -->
  <div class="subbar">SUBJECT OF INSURANCE</div>
  <div class="block">
    <div class="row">
      <div class="cell label w30 center">SUBJECT OF INSURANCE</div>
      <div class="cell label w15 center">AMOUNT</div>
      <div class="cell label w10 center">VALUATION</div>
      <div class="cell label w10 center">CO-INS (%)</div>
      <div class="cell label w15 center">CAUSE OF LOSS</div>
      <div class="cell label w10 center">DED</div>
      <div class="cell label w10 center">PREMIUM</div>
    </div>
    <!-- BUILDING — Amount from building_quote -->
    <div class="row">
      <div class="cell w30">BUILDING</div>
      <div class="cell w15 right">$<%= moneyUSD(data.building_quote) %></div>
      <div class="cell w10 center"><%= data.building_valuation || "R/C" %></div>
      <div class="cell w10 center"><%= data.building_coins || "80" %></div>
      <div class="cell w15 center"><%= data.building_cause || "SPECIAL" %></div>
      <div class="cell w10 center"><%= data.building_deductible || "$2,500" %></div>
      <div class="cell w10 right"><%= data.building_premium || "" %></div>
    </div>
    <!-- BUSINESS PERSONAL PROPERTY — Amount from business_personal_property, DED fixed $2,500 -->
    <div class="row">
      <div class="cell w30">BUSINESS PERSONAL PROPERTY</div>
      <div class="cell w15 right">$<%= moneyUSD(data.business_personal_property) %></div>
      <div class="cell w10 center"><%= data.bpp_valuation || "R/C" %></div>
      <div class="cell w10 center"><%= data.bpp_coins || "80" %></div>
      <div class="cell w15 center"><%= data.bpp_cause || "SPECIAL" %></div>
      <div class="cell w10 center">$2,500</div>
      <div class="cell w10 right"><%= data.bpp_premium || "" %></div>
    </div>
    <!-- BUSINESS INCOME / EXTRA EXPENSE -->
    <div class="row">
      <div class="cell w30">BUSINESS INCOME / EXTRA EXPENSE</div>
      <div class="cell w15 right">$<%= moneyUSD(data.business_income_amount) %></div>
      <div class="cell w10 center"><%= data.business_income_form || "" %></div>
      <div class="cell w10 center"><%= data.business_income_monthly_limit || "" %></div>
      <div class="cell w15 center"><%= data.business_income_extended || "" %></div>
      <div class="cell w10 center"><%= data.business_income_waiting_period || "" %></div>
      <div class="cell w10 right"></div>
    </div>
  </div>

  <!-- ADDITIONAL COVERAGES / OPTIONS / RESTRICTIONS -->
  <div class="subbar">ADDITIONAL COVERAGES / OPTIONS / RESTRICTIONS</div>
  <div class="block">
    <div class="row">
      <div class="cell w100 wrap" style="min-height:48px;">
        <%= data.addl_coverages_options_restrictions || "" %>
        <div><strong>Construction Type:</strong> <%= data.construction_type || "" %> &nbsp;&nbsp;
             <strong># Stories:</strong> <%= data.number_of_stories || "" %> &nbsp;&nbsp;
             <strong>Year Built:</strong> <%= data.year_built || "" %> &nbsp;&nbsp;
             <strong>Total Area (Sq Ft):</strong> <%= data.square_footage || "" %> &nbsp;&nbsp;
             <strong>% Sprinkler:</strong> <%= data.automatic_sprinkler || "" %></div>
      </div>
    </div>
  </div>

  <!-- ADDITIONAL INFORMATION -->
  <div class="subbar">ADDITIONAL INFORMATION</div>
  <div class="block">
    <div class="row">
      <div class="cell w100 wrap" style="min-height:42px;"><%= data.additional_information || "" %></div>
    </div>
  </div>

  <div class="footnote">ACORD 140 (2025/03) &nbsp;&nbsp; Page 1 of 3 &nbsp;&nbsp; © ACORD CORPORATION. All rights reserved.</div>
</div>

<!-- ====================== PAGE 2 ====================== -->
<div class="page">
  <div class="headerbar">
    <div class="acord">ACORD</div>
    <div class="title">PROPERTY SECTION — Attach to ACORD 125</div>
    <div class="formno">140</div>
  </div>

  <!-- CONSTRUCTION -->
  <div class="subbar">CONSTRUCTION</div>
  <div class="block">
    <div class="row">
      <div class="cell label w18">CONSTRUCTION TYPE</div>
      <div class="cell w22"><%= data.construction_type %></div>
      <div class="cell label w12"># OF STORIES</div>
      <div class="cell w10 center"><%= data.number_of_stories %></div>
      <div class="cell label w12">YEAR BUILT</div>
      <div class="cell w10 center"><%= data.year_built %></div>
      <div class="cell label w8">% SPRINK</div>
      <div class="cell w8 center"><%= data.automatic_sprinkler %></div>
    </div>
    <div class="row">
      <div class="cell label w18">TOTAL AREA (SQ FT)</div>
      <div class="cell w12 right"><%= data.square_footage %></div>
      <div class="cell label w12"># BASEMENTS</div>
      <div class="cell w8 center"><%= data.number_of_basements %></div>
      <div class="cell label w12">ROOF TYPE</div>
      <div class="cell w18"><%= data.roof_type %></div>
      <div class="cell label w10">WIND CLASS</div>
      <div class="cell w10 center"><%= data.wind_class %></div>
    </div>
    <div class="row">
      <div class="cell label w18">PROTECTION CLASS</div>
      <div class="cell w10 center"><%= data.protection_class %></div>
      <div class="cell label w18">DIST. TO HYDRANT (FT)</div>
      <div class="cell w10 center"><%= data.distance_hydrant_ft %></div>
      <div class="cell label w18">DIST. TO FIRE STATION (MI)</div>
      <div class="cell w10 center"><%= data.distance_firestation_mi %></div>
      <div class="cell label w6">GRADE</div>
      <div class="cell w10 center"><%= data.building_grade %></div>
    </div>
    <div class="row">
      <div class="cell label w18">BUILDING CODE</div>
      <div class="cell w22"><%= data.building_code %></div>
      <div class="cell label w20">OTHER OCCUPANCIES IN BLDG</div>
      <div class="cell w40 wrap"><%= data.other_occupancies %></div>
    </div>
  </div>

  <!-- PROTECTION / ALARMS -->
  <div class="subbar">PROTECTION / ALARMS</div>
  <div class="block">
    <div class="row">
      <div class="cell label w18">FIRE ALARM</div>
      <div class="cell w22"><%= data.fire_alarm_type %></div>
      <div class="cell label w12">MONITORED BY</div>
      <div class="cell w18"><%= data.fire_alarm_monitor %></div>
      <div class="cell label w12">MANUFACTURER</div>
      <div class="cell w18"><%= data.fire_alarm_manufacturer %></div>
    </div>
    <div class="row">
      <div class="cell label w18">BURGLAR ALARM</div>
      <div class="cell w22"><%= data.burglar_alarm_type %></div>
      <div class="cell label w12">CERT #</div>
      <div class="cell w18"><%= data.burglar_cert_number %></div>
      <div class="cell label w12">CERT EXP</div>
      <div class="cell w18"><%= data.burglar_cert_expiration %></div>
    </div>
    <div class="row">
      <div class="cell label w18">INSTALLER</div>
      <div class="cell w22"><%= data.burglar_alarm_installer %></div>
      <div class="cell label w18">ALARM WITH KEYS?</div>
      <div class="cell w12 center"><%= yn(data.burglar_alarm_keys) %></div>
      <div class="cell label w18">GUARDS / WATCHMEN</div>
      <div class="cell w12 center"><%= yn(data.guards_watchmen) %></div>
    </div>
    <div class="row">
      <div class="cell label w30">GUARD CLOCKED (EACH HOUR)?</div>
      <div class="cell w20 center"><%= yn(data.guard_clock_hourly) %></div>
      <div class="cell w50"></div>
    </div>
  </div>

  <!-- BUSINESS INCOME / VALUE REPORTING -->
  <div class="subbar">BUSINESS INCOME / VALUE REPORTING</div>
  <div class="block">
    <div class="row">
      <div class="cell label w25">VALUE REPORTING BASIS</div>
      <div class="cell w25"><%= data.value_reporting_basis %></div>
      <div class="cell label w25">REPORTING FREQUENCY</div>
      <div class="cell w25"><%= data.value_reporting_frequency %></div>
    </div>
    <div class="row">
      <div class="cell label w25">PROVISIONAL %</div>
      <div class="cell w25"><%= data.value_reporting_provisional_pct %></div>
      <div class="cell label w25">NOTES</div>
      <div class="cell w25 wrap"><%= data.additional_information %></div>
    </div>
  </div>

  <!-- REMARKS -->
  <div class="subbar">REMARKS (ACORD 101 MAY BE ATTACHED)</div>
  <div class="block">
    <div class="row">
      <div class="cell w100 wrap" style="min-height:50px;"><%= data.remarks %></div>
    </div>
  </div>

  <!-- ADDITIONAL INTEREST / CERTIFICATE RECIPIENT (moved to page 2) -->
  <div class="subbar">ADDITIONAL INTEREST / CERTIFICATE RECIPIENT</div>
  <div class="block">
    <div class="row">
      <div class="cell label w15">TYPE</div>
      <div class="cell w85 small">
        <span class="check"><%= (data.interests && data.interests[0] && String(data.interests[0].type||"").toUpperCase()==="ADDITIONAL_INSURED") ? "X" : "" %></span> ADDITIONAL INSURED
        <span class="check"><%= (data.interests && data.interests[0] && String(data.interests[0].type||"").toUpperCase()==="LOSS_PAYEE") ? "X" : "" %></span> LOSS PAYEE
        <span class="check"><%= (data.interests && data.interests[0] && String(data.interests[0].type||"").toUpperCase()==="MORTGAGEE") ? "X" : "" %></span> MORTGAGEE
        <span class="check"><%= (data.interests && data.interests[0] && String(data.interests[0].type||"").toUpperCase()==="LIENHOLDER") ? "X" : "" %></span> LIENHOLDER
        <span class="check"><%= (data.interests && data.interests[0] && String(data.interests[0].type||"").toUpperCase()==="LENDERS_LOSS") ? "X" : "" %></span> LENDER'S LOSS PAYABLE
      </div>
    </div>
    <div class="row">
      <div class="cell label w25">NAME AND ADDRESS</div>
      <div class="cell w35 wrap"><%= (data.interests && data.interests[0] && (data.interests[0].name||"")) %><br/><%= (data.interests && data.interests[0] && (data.interests[0].address||"")) %></div>
      <div class="cell label w15">RANK</div>
      <div class="cell w10"><%= (data.interests && data.interests[0] && (data.interests[0].rank||"")) %></div>
      <div class="cell label w15">REFERENCE / LOAN #</div>
      <div class="cell w10"><%= (data.interests && data.interests[0] && (data.interests[0].loanNumber||"")) %></div>
    </div>
    <div class="row">
      <div class="cell label w10">ITEM</div>
      <div class="cell w45">
        <span class="check"><%= (data.interests && data.interests[0] && String(data.interests[0].itemType||"").toUpperCase()==="LOCATION") ? "X" : "" %></span> LOCATION
        <span class="check"><%= (data.interests && data.interests[0] && String(data.interests[0].itemType||"").toUpperCase()==="BUILDING") ? "X" : "" %></span> BUILDING
        <span class="check"><%= (data.interests && data.interests[0] && String(data.interests[0].itemType||"").toUpperCase()==="VEHICLE") ? "X" : "" %></span> VEHICLE
        <span class="check"><%= (data.interests && data.interests[0] && String(data.interests[0].itemType||"").toUpperCase()==="BOAT") ? "X" : "" %></span> BOAT
      </div>
      <div class="cell label w20">ITEM DESCRIPTION</div>
      <div class="cell w25 wrap"><%= (data.interests && data.interests[0] && (data.interests[0].itemDescription||"")) %></div>
    </div>
  </div>

  <div class="footnote">ACORD 140 (2025/03) &nbsp;&nbsp; Page 2 of 3 &nbsp;&nbsp; © ACORD CORPORATION. All rights reserved.</div>
</div>

<!-- ====================== PAGE 3 ====================== -->
<div class="page">
  <div class="headerbar">
    <div class="acord">ACORD</div>
    <div class="title">PROPERTY SECTION — Attach to ACORD 125</div>
    <div class="formno">140</div>
  </div>

  <!-- STATE FRAUD NOTICES -->
  <div class="subbar">STATE FRAUD NOTICES</div>
  <div class="block">
    <div class="row">
      <div class="cell w100 small legal wrap" style="padding:4px;">
        <strong>Applicable in AL, AR, LA, MD, NM, RI and WV:</strong>
        Any person who knowingly (or willfully)* presents a false or fraudulent claim for payment of a loss or benefit or knowingly (or willfully)* presents false information in an application for insurance is guilty of a crime and may be subject to fines and confinement in prison. *Applies in MD Only.
        <br/><br/>
        <strong>Applicable in CA:</strong> For your protection, California law requires the following to appear on this form: Any person who knowingly presents false or fraudulent information to obtain or amend insurance coverage or to make a claim for the payment of a loss is guilty of a crime and may be subject to fines and confinement in state prison.
        <br/><br/>
        <strong>Applicable in CO:</strong> It is unlawful to knowingly provide false, incomplete, or misleading facts or information to an insurance company for the purpose of defrauding or attempting to defraud the company. Penalties may include imprisonment, fines, denial of insurance and civil damages. Any insurance company or agent of an insurance company who knowingly provides false, incomplete, or misleading facts or information to a policyholder or claimant for the purpose of defrauding or attempting to defraud the policyholder or claimant with regard to a settlement or award payable from insurance proceeds shall be reported to the Colorado Division of Insurance within the Department of Regulatory Agencies.
        <br/><br/>
        <strong>Applicable in DC:</strong> WARNING: It is a crime to provide false or misleading information to an insurer for the purpose of defrauding the insurer or any other person. Penalties include imprisonment and/or fines. In addition, an insurer may deny insurance benefits if false information materially related to a claim was provided by the applicant.
        <br/><br/>
        <strong>Applicable in FL and OK:</strong> Any person who knowingly and with intent to injure, defraud, or deceive any insurer files a statement of claim or an application containing any false, incomplete, or misleading information is guilty of a felony (of the third degree)*. *Applies in FL Only.
        <br/><br/>
        <strong>Applicable in KS:</strong> Any person who, knowingly and with intent to defraud, presents, causes to be presented or prepares with knowledge or belief that it will be presented to or by an insurer, purported insurer, broker or any agent thereof, any written, electronic, electronic impulse, facsimile, magnetic, oral, or telephonic communication or statement as part of, or in support of, an application for the issuance of, or the rating of an insurance policy for personal or commercial insurance, or a claim for payment or other benefit pursuant to an insurance policy for commercial or personal insurance which such person knows to contain materially false information concerning any fact material thereto; or conceals, for the purpose of misleading, information concerning any fact material thereto commits a fraudulent insurance act.
        <br/><br/>
        <strong>Applicable in KY, OH and PA:</strong> Any person who knowingly and with intent to defraud any insurance company or other person files an application for insurance or statement of claim containing any materially false information or conceals for the purpose of misleading, information concerning any fact material thereto commits a fraudulent insurance act, which is a crime and subjects such person to criminal and civil penalties.
        <br/><br/>
        <strong>Applicable in ME, TN, VA and WA:</strong> It is a crime to knowingly provide false, incomplete or misleading information to an insurance company for the purpose of defrauding the company. Penalties (may)* include imprisonment, fines and denial of insurance benefits. *Applies in ME Only.
        <br/><br/>
        <strong>Applicable in NJ:</strong> Any person who includes any false or misleading information on an application for an insurance policy is subject to criminal and civil penalties.
        <br/><br/>
        <strong>Applicable in NY:</strong> Any person who knowingly and with intent to defraud any insurance company or other person files an application for insurance or statement of claim containing any materially false information, or conceals for the purpose of misleading, information concerning any fact material thereto, commits a fraudulent insurance act, which is a crime, and shall also be subject to a civil penalty not to exceed five thousand dollars and the stated value of the claim for each such violation.
        <br/><br/>
        <strong>Applicable in OR:</strong> Any person who knowingly and with intent to defraud or solicit another to defraud the insurer by submitting an application containing a false statement as to any material fact may be violating state law.
        <br/><br/>
        <strong>Applicable in PR:</strong> Any person who knowingly and with the intention of defrauding presents false information in an insurance application, or presents, helps, or causes the presentation of a fraudulent claim for the payment of a loss or any other benefit, or presents more than one claim for the same damage or loss, shall incur a felony and, upon conviction, shall be sanctioned for each violation by a fine of not less than five thousand dollars ($5,000) and not more than ten thousand dollars ($10,000), or a fixed term of imprisonment for three (3) years, or both penalties. Should aggravating circumstances be present, the penalty thus established may be increased to a maximum of five (5) years, if extenuating circumstances are present, it may be reduced to a minimum of two (2) years.
      </div>
    </div>
  </div>

