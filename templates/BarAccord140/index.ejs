<%
/* ===== Minimal helpers & defaults (safe) ===== */
var defaults = {
  // Producer / Agency
  producerName: "All Access Ins, dba Commercial Insurance Direct LLC",
  producerAddress1: "9200 W Cross Drive #515",
  producerAddress2: "Littleton, CO 80123",
  producerContact: "Rick Cline or Gerry Jones",
  producerPhone: "(555) 123-4567",
  producerFax: "(555) 123-4568",
  producerEmail: "quote@barinsurancedirect.com",

  // Header / Policy meta
  date: "",
  effectiveDate: "",
  carrier: "",
  naicCode: "",
  policyNumber: "",
  agencyCustomerId: "",

  // Applicant / Named Insured
  applicant_name: "",
  premises_name: "",
  premise_address: "",
  premise_city: "",
  premise_state: "",
  premise_zip: "",

  // Premises info (page 1)
  premises_number: "1",
  building_number: "1",
  bldg_description: "",
  fire_district: "",
  fire_code: "",
  exposure_front: "",
  exposure_right: "",
  exposure_left: "",
  exposure_rear: "",
  dist_front: "",
  dist_right: "",
  dist_left: "",
  dist_rear: "",

  // Subject of Insurance amounts (your requested mappings)
  building_quote: "",
  business_personal_property: "",

  // Construction / Protection (page 2)
  construction_type: "",
  year_built: "",
  number_of_stories: "",
  square_footage: "",
  automatic_sprinkler: "", // % or Y/N text you pass
  roof_type: "",
  wiring_year: "",
  roofing_year: "",
  plumbing_year: "",
  heating_year: "",

  // Protection / Alarms
  protection_class: "",
  burglar_alarm_type: "",
  burglar_alarm_cert: "",
  burglar_alarm_exp: "",
  central_station: "",
  fire_alarm_type: "",
  fire_alarm_cert: "",
  fire_alarm_exp: "",
  with_keys: "",
  guards_clock_hourly: "",

  // Business Income / Reporting
  business_income_coverage: "",
  business_income_limit: "",
  business_income_months: "",
  extra_expense_limit: "",
  reporting_required: "",

  // Additional coverages/options/restrictions (common rows)
  spoilage_coverage: "",
  spoilage_ded: "",
  spoilage_yes: "",
  breakdown_contam: "",
  breakdown_ded: "",
  breakdown_yes: "",
  refrig_maint_agreement: "",
  refrig_maint_yes: "",
  power_outage: "",
  power_outage_ded: "",
  power_outage_yes: "",
  ordinance_or_law: "",
  ordinance_limit: "",
  ordinance_ded: "",
  ordinance_yes: "",

  // Accept / Reject bands (mine subsidence / sinkhole)
  mine_subsidence_accept: "",
  mine_subsidence_reject: "",
  sinkhole_accept: "",
  sinkhole_reject: "",

  // Additional Premises info
  addl_premises_notes: "",

  // Additional Interests (we’ll show first two on page 2)
  interests: [
    // { type:"ADDITIONAL_INSURED|LOSS_PAYEE|MORTGAGEE|LIENHOLDER|LENDERS_LOSS",
    //   name:"", address:"", city:"", state:"", zip:"",
    //   rank:"", evidence:"", loanNumber:"",
    //   itemType:"LOCATION|BUILDING|VEHICLE|BOAT|EQUIPMENT",
    //   itemClass:"", itemDescription:""
    // }
  ],

  // Remarks (page 2)
  remarks: ""
};

var data = Object.assign({}, defaults, (typeof formData === "object" && formData) || {});

function yes(v){
  var s = String(v == null ? "" : v).toLowerCase();
  return s==="yes"||s==="true"||s==="1"||s==="on"||s==="y"||v===true||v===1;
}
function money(v){
  if (v === null || v === undefined || v === "") return "";
  var n = Number(String(v).replace(/[^0-9.-]/g,""));
  if (isNaN(n)) return String(v);
  return n.toLocaleString("en-US",{minimumFractionDigits:0,maximumFractionDigits:0});
}
function formatDate(d){
  try{
    if(!d) return "";
    var dt = new Date(d);
    if(isNaN(dt.getTime())) return String(d);
    var mm=("0"+(dt.getMonth()+1)).slice(-2);
    var dd=("0"+dt.getDate()).slice(-2);
    var yy=dt.getFullYear();
    return mm+"/"+dd+"/"+yy;
  }catch(e){ return String(d||""); }
}
%>
<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8"/>
  <title>ACORD 140 - Property Section</title>
  <style>
    /* ===== Shared layout ===== */
    @media print { @page { margin: 0.25in; size: letter; } .page { page-break-after: always; } .page:last-child{ page-break-after: auto; } }
    * { box-sizing: border-box; margin:0; padding:0; }
    body { font-family: Arial, sans-serif; font-size: 8pt; line-height:1.1; color:#000; }
    .page { width:100%; min-height:10.5in; padding:0.125in; border:1px solid #ccc; position:relative; }
    .headerbar{ display:flex; border:2px solid #000; background:#fff; margin-bottom:2px; align-items:center; height:40px; }
    .headerbar .acord, .headerbar .formno { background:#000; color:#fff; font-weight:bold; font-size:16pt; width:60px; text-align:center; height:100%; display:flex; align-items:center; justify-content:center; }
    .headerbar .acord{ width:80px; }
    .headerbar .title{ flex:1; font-weight:bold; font-size:12pt; text-align:center; padding:0 10px; }
    .subbar{ background:#000; color:#fff; font-weight:bold; font-size:9pt; padding:3px 8px; margin:2px 0; text-align:left; }
    .block{ border:1px solid #000; margin-bottom:2px; }
    .row{ display:flex; border-bottom:1px solid #000; min-height:18px; }
    .row:last-child{ border-bottom:none; }
    .cell{ border-right:1px solid #000; padding:1px 3px; display:flex; align-items:center; overflow-wrap:break-word; }
    .cell:last-child{ border-right:none; }
    .cell.label{ background:#f0f0f0; font-weight:bold; font-size:6.5pt; }
    .cell.center{ justify-content:center; text-align:center; }
    .cell.right{ justify-content:flex-end; text-align:right; }
    .cell.wrap{ flex-wrap:wrap; align-items:flex-start; }
    .cell.small{ font-size:7pt; }
    .cell.legal{ font-size:6pt; line-height:1.2; }
    .check{ display:inline-block; width:10px; height:10px; border:1px solid #000; text-align:center; line-height:8px; font-size:8pt; margin-right:5px; vertical-align:middle; }
    .check-xs{ width:8px; height:8px; font-size:6pt; line-height:6px; }
    .footnote{ position:absolute; bottom:0.125in; left:0.125in; right:0.125in; font-size:6pt; border-top:1px solid #000; padding-top:2px; text-align:center; }
    .w5{width:5%}.w6{width:6%}.w7{width:7%}.w8{width:8%}.w9{width:9%}.w10{width:10%}.w12{width:12%}.w13{width:13%}.w14{width:14%}.w15{width:15%}.w18{width:18%}.w20{width:20%}.w22{width:22%}.w25{width:25%}.w28{width:28%}.w30{width:30%}.w33{width:33%}.w35{width:35%}.w40{width:40%}.w45{width:45%}.w50{width:50%}.w60{width:60%}.w65{width:65%}.w70{width:70%}.w75{width:75%}.w80{width:80%}.w85{width:85%}.w90{width:90%}.w100{width:100%}
    @media screen{ .page{ margin-bottom:20px; box-shadow:0 0 10px rgba(0,0,0,.1);} }
  </style>
</head>
<body>

<!-- ====================== PAGE 1 ====================== -->
<div class="page">
  <div class="headerbar">
    <div class="acord">ACORD</div>
    <div class="title">PROPERTY SECTION</div>
    <div class="formno">140</div>
  </div>

  <!-- PRODUCER / APPLICANT -->
  <div class="subbar">PRODUCER / APPLICANT</div>
  <div class="block">
    <div class="row">
      <div class="cell label w15">AGENCY</div>
      <div class="cell w35 wrap">
        <%= data.producerName %><br/>
        <%= data.producerAddress1 %><br/>
        <%= data.producerAddress2 %>
      </div>
      <div class="cell label w15">AGENCY CUSTOMER ID</div>
      <div class="cell w15"><%= data.agencyCustomerId %></div>
      <div class="cell label w10">DATE (MM/DD/YYYY)</div>
      <div class="cell w10"><%= formatDate(data.date) %></div>
    </div>
    <div class="row">
      <div class="cell label w20">APPLICANT / FIRST NAMED INSURED</div>
      <div class="cell w40 wrap"><%= data.applicant_name %></div>
      <div class="cell label w10">CARRIER</div>
      <div class="cell w10"><%= data.carrier %></div>
      <div class="cell label w10">NAIC CODE</div>
      <div class="cell w10"><%= data.naicCode %></div>
    </div>
    <div class="row">
      <div class="cell label w20">POLICY NUMBER</div>
      <div class="cell w20"><%= data.policyNumber %></div>
      <div class="cell label w20">EFFECTIVE DATE</div>
      <div class="cell w20"><%= formatDate(data.effectiveDate) %></div>
      <div class="cell label w10">PREMISES #</div>
      <div class="cell w10"><%= data.premises_number %></div>
    </div>
  </div>

  <!-- PREMISES INFORMATION -->
  <div class="subbar">PREMISES INFORMATION</div>
  <div class="block">
    <div class="row">
      <div class="cell label w15">BUILDING #</div>
      <div class="cell w10"><%= data.building_number %></div>
      <div class="cell label w15">BUILDING DESCRIPTION</div>
      <div class="cell w60 wrap"><%= data.bldg_description %></div>
    </div>
    <div class="row">
      <div class="cell label w20">STREET ADDRESS</div>
      <div class="cell w80 wrap"><%= [data.premise_address, data.premise_city && (data.premise_city + ", " + data.premise_state + " " + data.premise_zip)].filter(Boolean).join(" • ") %></div>
    </div>
    <div class="row">
      <div class="cell label w20">FIRE DISTRICT / CODE #</div>
      <div class="cell w30"><%= [data.fire_district, data.fire_code].filter(Boolean).join(" / ") %></div>
      <div class="cell label w25">PROTECTION CLASS</div>
      <div class="cell w25"><%= data.protection_class %></div>
    </div>
    <div class="row">
      <div class="cell label w15">EXPOSURE</div>
      <div class="cell label w20 center">FRONT</div>
      <div class="cell label w20 center">RIGHT</div>
      <div class="cell label w20 center">LEFT</div>
      <div class="cell label w25 center">REAR</div>
    </div>
    <div class="row">
      <div class="cell label w15">ADJACENT / DISTANCE</div>
      <div class="cell w20 center"><%= data.exposure_front %><% if (data.dist_front) { %> — <%= data.dist_front %> ft<% } %></div>
      <div class="cell w20 center"><%= data.exposure_right %><% if (data.dist_right) { %> — <%= data.dist_right %> ft<% } %></div>
      <div class="cell w20 center"><%= data.exposure_left %><% if (data.dist_left) { %> — <%= data.dist_left %> ft<% } %></div>
      <div class="cell w25 center"><%= data.exposure_rear %><% if (data.dist_rear) { %> — <%= data.dist_rear %> ft<% } %></div>
    </div>
  </div>

  <!-- SUBJECT OF INSURANCE -->
  <div class="subbar">SUBJECT OF INSURANCE</div>
  <div class="block">
    <div class="row">
      <div class="cell label w18 center">SUBJECT</div>
      <div class="cell label w12 center">AMOUNT</div>
      <div class="cell label w9 center">VALUATION</div>
      <div class="cell label w8 center">CO-INS</div>
      <div class="cell label w10 center">CAUSE OF LOSS</div>
      <div class="cell label w9 center">DED</div>
      <div class="cell label w9 center">PREMIUM</div>
      <div class="cell label w8 center">BLKT #</div>
      <div class="cell label w9 center">INFL %</div>
      <div class="cell label w8 center">TYPE OF DED</div>
    </div>

    <!-- Building -->
    <div class="row">
      <div class="cell w18">BUILDING</div>
      <div class="cell w12 right">$<%= money(data.building_quote) %></div>
      <div class="cell w9 center"></div>
      <div class="cell w8 center"></div>
      <div class="cell w10 center"></div>
      <div class="cell w9 center"></div>
      <div class="cell w9 right"></div>
      <div class="cell w8 center"></div>
      <div class="cell w9 center"></div>
      <div class="cell w8 center"></div>
    </div>

    <!-- Business Personal Property (with hard-coded $2,500 Deductible) -->
    <div class="row">
      <div class="cell w18">BUSINESS PERSONAL PROPERTY</div>
      <div class="cell w12 right">$<%= money(data.business_personal_property) %></div>
      <div class="cell w9 center"></div>
      <div class="cell w8 center"></div>
      <div class="cell w10 center"></div>
      <div class="cell w9 center">$2,500</div>
      <div class="cell w9 right"></div>
      <div class="cell w8 center"></div>
      <div class="cell w9 center"></div>
      <div class="cell w8 center"></div>
    </div>

    <!-- Business Income / Extra Expense (blank placeholders) -->
    <div class="row">
      <div class="cell w18">BUSINESS INCOME</div>
      <div class="cell w12 right">$<%= money(data.business_income_limit) %></div>
      <div class="cell w9 center"></div>
      <div class="cell w8 center"></div>
      <div class="cell w10 center"></div>
      <div class="cell w9 center"></div>
      <div class="cell w9 right"></div>
      <div class="cell w8 center"></div>
      <div class="cell w9 center"></div>
      <div class="cell w8 center"></div>
    </div>

    <!-- Accept / Reject bars -->
    <div class="row">
      <div class="cell label w25">MINE SUBSIDENCE</div>
      <div class="cell w15"><span class="check"><%= yes(data.mine_subsidence_accept) ? "X" : "" %></span> ACCEPT</div>
      <div class="cell w15"><span class="check"><%= yes(data.mine_subsidence_reject) ? "X" : "" %></span> REJECT</div>
      <div class="cell label w25">SINKHOLE</div>
      <div class="cell w10"><span class="check"><%= yes(data.sinkhole_accept) ? "X" : "" %></span> ACCEPT</div>
      <div class="cell w10"><span class="check"><%= yes(data.sinkhole_reject) ? "X" : "" %></span> REJECT</div>
    </div>
  </div>

  <!-- ADDITIONAL COVERAGES / OPTIONS / RESTRICTIONS -->
  <div class="subbar">ADDITIONAL COVERAGES / OPTIONS / RESTRICTIONS</div>
  <div class="block">
    <div class="row">
      <div class="cell label w25">SPOILAGE COVERAGE</div>
      <div class="cell w20">LIMIT: $<%= money(data.spoilage_coverage) %></div>
      <div class="cell w20">DED: $<%= money(data.spoilage_ded) %></div>
      <div class="cell w35">APPLY? <span class="check"><%= yes(data.spoilage_yes) ? "X" : "" %></span></div>
    </div>
    <div class="row">
      <div class="cell label w25">BREAKDOWN / CONTAMINATION</div>
      <div class="cell w20">LIMIT: $<%= money(data.breakdown_contam) %></div>
      <div class="cell w20">DED: $<%= money(data.breakdown_ded) %></div>
      <div class="cell w35">APPLY? <span class="check"><%= yes(data.breakdown_yes) ? "X" : "" %></span></div>
    </div>
    <div class="row">
      <div class="cell label w25">REFRIGERATION MAINT AGREEMENT</div>
      <div class="cell w35">AGREEMENT #: <%= data.refrig_maint_agreement %></div>
      <div class="cell w40">IN FORCE? <span class="check"><%= yes(data.refrig_maint_yes) ? "X" : "" %></span></div>
    </div>
    <div class="row">
      <div class="cell label w25">POWER OUTAGE (OFF PREMISES)</div>
      <div class="cell w20">LIMIT: $<%= money(data.power_outage) %></div>
      <div class="cell w20">DED: $<%= money(data.power_outage_ded) %></div>
      <div class="cell w35">APPLY? <span class="check"><%= yes(data.power_outage_yes) ? "X" : "" %></span></div>
    </div>
    <div class="row">
      <div class="cell label w25">ORDINANCE OR LAW</div>
      <div class="cell w20">LIMIT: $<%= money(data.ordinance_limit) %></div>
      <div class="cell w20">DED: $<%= money(data.ordinance_ded) %></div>
      <div class="cell w35">APPLY? <span class="check"><%= yes(data.ordinance_yes) ? "X" : "" %></span></div>
    </div>
  </div>

  <div class="footnote">ACORD 140 (Bar Layout) &nbsp;&nbsp;&nbsp; Page 1 of 3</div>
</div>

<!-- ====================== PAGE 2 ====================== -->
<div class="page">
  <div class="headerbar">
    <div class="acord">ACORD</div>
    <div class="title">PROPERTY SECTION</div>
    <div class="formno">140</div>
  </div>

  <!-- CONSTRUCTION DETAILS -->
  <div class="subbar">CONSTRUCTION DETAILS</div>
  <div class="block">
    <div class="row">
      <div class="cell label w20">CONSTRUCTION TYPE</div>
      <div class="cell w20"><%= data.construction_type %></div>
      <div class="cell label w15">YEAR BUILT</div>
      <div class="cell w10"><%= data.year_built %></div>
      <div class="cell label w15"># OF STORIES</div>
      <div class="cell w10"><%= data.number_of_stories %></div>
      <div class="cell label w10">% SPRINK</div>
      <div class="cell w10"><%= data.automatic_sprinkler %></div>
    </div>
    <div class="row">
      <div class="cell label w20">TOTAL AREA (SQ FT)</div>
      <div class="cell w20 right"><%= data.square_footage %></div>
      <div class="cell label w15">ROOF TYPE</div>
      <div class="cell w15"><%= data.roof_type %></div>
      <div class="cell label w15">PROTECTION CLASS</div>
      <div class="cell w15"><%= data.protection_class %></div>
    </div>
    <div class="row">
      <div class="cell label w20">WIRING (YR)</div>
      <div class="cell w10"><%= data.wiring_year %></div>
      <div class="cell label w20">ROOFING (YR)</div>
      <div class="cell w10"><%= data.roofing_year %></div>
      <div class="cell label w20">PLUMBING (YR)</div>
      <div class="cell w10"><%= data.plumbing_year %></div>
      <div class="cell label w20">HEATING (YR)</div>
      <div class="cell w10"><%= data.heating_year %></div>
    </div>
  </div>

  <!-- PROTECTION / ALARMS -->
  <div class="subbar">PROTECTION / ALARMS</div>
  <div class="block">
    <div class="row">
      <div class="cell label w20">BURGLAR ALARM</div>
      <div class="cell w20"><%= data.burglar_alarm_type %></div>
      <div class="cell label w15">CERT #</div>
      <div class="cell w15"><%= data.burglar_alarm_cert %></div>
      <div class="cell label w15">EXP</div>
      <div class="cell w15"><%= data.burglar_alarm_exp %></div>
    </div>
    <div class="row">
      <div class="cell label w20">FIRE ALARM</div>
      <div class="cell w20"><%= data.fire_alarm_type %></div>
      <div class="cell label w15">CERT #</div>
      <div class="cell w15"><%= data.fire_alarm_cert %></div>
      <div class="cell label w15">EXP</div>
      <div class="cell w15"><%= data.fire_alarm_exp %></div>
    </div>
    <div class="row">
      <div class="cell label w25">CENTRAL STATION</div>
      <div class="cell w25"><%= data.central_station %></div>
      <div class="cell label w25">WITH KEYS?</div>
      <div class="cell w10"><span class="check"><%= yes(data.with_keys) ? "X" : "" %></span></div>
      <div class="cell label w15">GUARDS/CLOCK HOURLY?</div>
      <div class="cell w10"><span class="check"><%= yes(data.guards_clock_hourly) ? "X" : "" %></span></div>
    </div>
  </div>

  <!-- BUSINESS INCOME / VALUE REPORTING -->
  <div class="subbar">BUSINESS INCOME / VALUE REPORTING</div>
  <div class="block">
    <div class="row">
      <div class="cell label w25">BUSINESS INCOME COVERAGE</div>
      <div class="cell w25"><%= data.business_income_coverage %></div>
      <div class="cell label w25"># OF MONTHS</div>
      <div class="cell w25"><%= data.business_income_months %></div>
    </div>
    <div class="row">
      <div class="cell label w25">EXTRA EXPENSE LIMIT</div>
      <div class="cell w25 right">$<%= money(data.extra_expense_limit) %></div>
      <div class="cell label w25">REPORTING REQUIRED?</div>
      <div class="cell w25"><span class="check"><%= yes(data.reporting_required) ? "X" : "" %></span></div>
    </div>
  </div>

  <!-- ADDITIONAL PREMISES INFORMATION -->
  <div class="subbar">ADDITIONAL PREMISES INFORMATION</div>
  <div class="block">
    <div class="row">
      <div class="cell w100 wrap" style="min-height:36px;"><%= data.addl_premises_notes %></div>
    </div>
  </div>

  <!-- ADDITIONAL INTEREST / CERTIFICATE RECIPIENT (ENDS PAGE 2) -->
  <div class="subbar">ADDITIONAL INTEREST / CERTIFICATE RECIPIENT</div>
  <div class="block">
    <% var A = Array.isArray(data.interests) ? data.interests.slice(0,2) : []; %>
    <% A.forEach(function(I,idx){ %>
      <div class="row">
        <div class="cell label w10">TYPE</div>
        <div class="cell w40">
          <span class="check"><%= (String(I.type||"").toUpperCase()==="ADDITIONAL_INSURED") ? "X" : "" %></span> ADDITIONAL INSURED
          <span class="check"><%= (String(I.type||"").toUpperCase()==="LOSS_PAYEE") ? "X" : "" %></span> LOSS PAYEE
          <span class="check"><%= (String(I.type||"").toUpperCase()==="MORTGAGEE") ? "X" : "" %></span> MORTGAGEE
          <span class="check"><%= (String(I.type||"").toUpperCase()==="LIENHOLDER") ? "X" : "" %></span> LIENHOLDER
          <span class="check"><%= (String(I.type||"").toUpperCase()==="LENDERS_LOSS") ? "X" : "" %></span> LENDER'S LOSS PAYABLE
        </div>
        <div class="cell label w10">RANK</div>
        <div class="cell w10"><%= I.rank || "" %></div>
        <div class="cell label w10">EVIDENCE</div>
        <div class="cell w10"><%= I.evidence || "" %></div>
      </div>
      <div class="row">
        <div class="cell label w15">NAME & ADDRESS</div>
        <div class="cell w45 wrap">
          <%= I.name || "" %><br/>
          <%= I.address || "" %><% if (I.city){ %><br/><%= I.city %>, <%= I.state || "" %> <%= I.zip || "" %><% } %>
        </div>
        <div class="cell label w15">REFERENCE / LOAN #</div>
        <div class="cell w25"><%= I.loanNumber || "" %></div>
      </div>
      <div class="row">
        <div class="cell label w10">ITEM</div>
        <div class="cell w40">
          <span class="check"><%= (String(I.itemType||"").toUpperCase()==="LOCATION") ? "X" : "" %></span> LOCATION
          <span class="check"><%= (String(I.itemType||"").toUpperCase()==="BUILDING") ? "X" : "" %></span> BUILDING
          <span class="check"><%= (String(I.itemType||"").toUpperCase()==="VEHICLE") ? "X" : "" %></span> VEHICLE
          <span class="check"><%= (String(I.itemType||"").toUpperCase()==="BOAT") ? "X" : "" %></span> BOAT
          <span class="check"><%= (String(I.itemType||"").toUpperCase()==="EQUIPMENT") ? "X" : "" %></span> EQUIPMENT
        </div>
        <div class="cell label w15">ITEM CLASS</div>
        <div class="cell w15"><%= I.itemClass || "" %></div>
        <div class="cell label w10">ITEM DESCRIPTION</div>
